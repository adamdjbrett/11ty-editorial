<div class="row">
{%- for post in postslist | reverse %}
	<div class="col-md-4 p-3">
		<h3><a href="{{ post.url }}" class="postlist-link">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a></h3>
		<p>{{post.data.description}}</p>
	</div>
{%- endfor %}
</div>
